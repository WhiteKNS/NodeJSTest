{
  "name": "testnodejs",
  "version": "0.1.0",
  "description": "test NodeJS project",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist",
    "serve": "npm run build && node dist/index.js",
    "precommit": "yarn run lint",
    "test:e2e": "cucumber-js test/spec/cucumber/features --require-module @babel/register --require test/spec/cucumber/steps",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "watch": "nodemon -w src --exec yarn run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WhiteKNS/NodeJSTest.git"
  },
  "keywords": [
    "\"test",
    "learning",
    "NodeJS\""
  ],
  "author": "Nataliia K",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WhiteKNS/NodeJSTest/issues"
  },
  "homepage": "https://github.com/WhiteKNS/NodeJSTest#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "cucumber": "^7.0.0-rc.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "ndb": "^1.1.5",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "superagent": "^6.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1"
  }
}
