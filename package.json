{
  "name": "testnodejs",
  "version": "0.1.0",
  "description": "test NodeJS project",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist --copy-files",
    "serve": "yarn run build && dotenv -e envs/.env node dist/index.js",
    "precommit": "yarn run lint",
    "test:e2e": "dotenv -e envs/test.env -e envs/.env cucumber-js -- test/spec/cucumber/features --require-module @babel/register --require test/spec/cucumber/steps",
    "test:unit": "npx mocha 'src/**/*.test.js' --require @babel/register",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:integration": "dotenv -e envs/test.env -e envs/.env mocha -- 'src/**/*.integration.test.js' --require @babel/register",
    "test:unit:coverage": "nyc --reporter=html --reporter=text yarn run test:unit",
    "test:integration:coverage": "nyc --reporter=html --reporter=text yarn run test:integration",
    "test:e2e:coverage": "nyc --reporter=html --reporter=text yarn run test:e2e",
    "test:serve": "dotenv -e envs/test.env -e envs/.env babel-node src/index.js",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "watch": "nodemon -w src --exec yarn run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WhiteKNS/NodeJSTest.git"
  },
  "keywords": [
    "\"test",
    "learning",
    "NodeJS\""
  ],
  "author": "Nataliia K",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WhiteKNS/NodeJSTest/issues"
  },
  "homepage": "https://github.com/WhiteKNS/NodeJSTest#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "cucumber": "^7.0.0-rc.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "ajv": "^7.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "elasticsearch": "^16.7.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "latest-version": "^5.1.0",
    "random-seed": "^0.3.0",
    "special-escape": "^0.1.1",
    "superagent": "^6.1.0"
  }
}
